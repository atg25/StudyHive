{
  "name": "studyhive-server",
  "version": "1.0.0",
  "description": "StudyHive AI-powered study platform backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "cleanup": "node scripts/cleanup-audio.js",
    "validate": "node -e \"require('./scripts/validate-env').validateEnvironment()\"",
    "pre-deploy": "./scripts/pre-deploy-check.sh",
    "update-deps": "npm update @google/generative-ai dotenv",
    "list-voices": "node utils/list-voices.js",
    "list-models": "node utils/list-models.js"
  },
  "keywords": [
    "ai",
    "education",
    "study",
    "gemini"
  ],
  "author": "StudyHive Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
